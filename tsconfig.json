{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "stripInternal": false,
    "importHelpers": true,
    "target": "es5",
    "lib": [
      "es5",
      "es2015",
      "dom"
    ],
    "baseUrl": "./src",
    "sourceRoot": "./src",
    "paths": {
      "@reactgular/observables": [
        "./"
      ],
      "@reactgular/observables/*": [
        "./*"
      ]
    }
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  },
  "files": [
    "./src/index.ts"
  ]
}
